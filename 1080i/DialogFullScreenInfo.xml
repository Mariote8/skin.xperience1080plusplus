<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">5</defaultcontrol>
	<onunload>ClearProperty(ShowNext,seekbar)</onunload>
	<onunload>ClearProperty(ExtendPlot,seekbar)</onunload>
	<zorder>3</zorder>
	<controls>
		<control type="group">
			<animation type="WindowOpen" condition="Player.Paused | Player.ShowInfo">
				<effect type="fade" start="0" end="100" time="200" tween="sine" easing="inout" />
			</animation>
			<animation type="WindowOpen" condition="!Player.Paused + !Player.ShowInfo">
				<effect type="fade" start="0" end="100" time="200" tween="sine" easing="inout" delay="300" />
			</animation>
			<animation type="WindowClose">
				<effect type="fade" start="100" end="0" time="200" tween="sine" easing="in" />
			</animation>
			<animation type="Visible" reversible="false">
				<effect type="fade" start="0" end="100" time="300" tween="sine" easing="inout" delay="150" />
			</animation>
			<animation type="Hidden" reversible="false">
				<effect type="fade" start="100" end="0" time="200" tween="sine" easing="out" />
			</animation>
			<animation effect="slide" start="0,0" end="0,205" time="300" tween="cubic" easing="inout"
				condition="Window.IsActive(videoosd) | [!Window.IsActive(videoosd) + [Window.IsActive(DialogSubtitles.xml) | Window.IsActive(pvrguideinfo) | Window.IsActive(pvrosdchannels) | Window.IsActive(pvrosdguide) | Window.IsActive(yesnodialog) | Window.IsActive(okdialog) | Window.IsActive(progressdialog) | Window.IsActive(virtualkeyboard) | Window.IsActive(numericinput) | Window.IsActive(shutdownmenu) | Window.IsActive(osdvideosettings) | Window.IsActive(osdaudiosettings) | Window.IsActive(videobookmarks)]]">Conditional</animation>
			<visible>!Window.IsActive(videoosd) + ![!Window.IsActive(videoosd) + [Window.IsActive(DialogSubtitles.xml) | Window.IsActive(pvrguideinfo) | Window.IsActive(pvrosdchannels) | Window.IsActive(pvrosdguide) | Window.IsActive(yesnodialog) | Window.IsActive(okdialog) | Window.IsActive(progressdialog) | Window.IsActive(virtualkeyboard) | Window.IsActive(numericinput) | Window.IsActive(shutdownmenu) | Window.IsActive(osdvideosettings) | Window.IsActive(osdaudiosettings) |
				Window.IsActive(videobookmarks)]]
			</visible>
			<control type="grouplist" id="5">
				<posx>1617</posx>
				<posy>723</posy>
				<width>300</width>
				<height>100</height>
				<align>right</align>
				<orientation>horizontal</orientation>
				<itemgap>-40</itemgap>
				<onup condition="IsEmpty(Window(seekbar).Property(ExtendPlot))">SetProperty(ExtendPlot,1,seekbar)</onup>
				<ondown condition="!IsEmpty(Window(seekbar).Property(ExtendPlot))">ClearProperty(ExtendPlot,seekbar)</ondown>
				<usecontrolcoords>true</usecontrolcoords>
			</control>
			<control type="label">
				<width>400</width>
				<height>44</height>
				<label>$INFO[Player.Chapter,$LOCALIZE[21396] , / ]$INFO[Player.ChapterCount]</label>
				<textcolor>aawhite</textcolor>
				<font>Font-Condensed-S36</font>
				<align>right</align>
				<aligny>center</aligny>
				<visible>IntegerGreaterThan(Player.ChapterCount,0)</visible>
				<visible>!IsEmpty(VideoPlayer.Plot) + !IsEmpty(Window(seekbar).Property(ExtendPlot))</visible>
				<include>visiblehidefadeinfo</include>
			</control>
			<control type="group">
				<visible>!IsEmpty(VideoPlayer.Rating) + Skin.HasSetting(UseAlternateFlags.Enabled)</visible>
				<include>visiblehidefadeinfo</include>
				<posx>1710</posx>
				<posy>865</posy>
				<control type="image">
					<posx>2</posx>
					<posy>-116</posy>
					<width>108</width>
					<height>49</height>
					<aspect>keepaspect</aspect>
					<texture>colourflags/posters/imdb.png</texture>
					<visible>!Skin.HasSetting(UseNonColourFlags.Enabled)</visible>
				</control>
				<control type="image">
					<posx>2</posx>
					<posy>-116</posy>
					<width>108</width>
					<height>49</height>
					<aspect>keepaspect</aspect>
					<texture colordiffuse="mod-infotags">noncolourflags/posters/imdb.png</texture>
					<visible>Skin.HasSetting(UseNonColourFlags.Enabled)</visible>
				</control>
				<control type="label">
					<posx>89</posx>
					<posy>-122</posy>
					<width>80</width>
					<height>58</height>
					<font>Font-Condensed-S36</font>
					<label fallback="                 •••">$INFO[VideoPlayer.Rating, ,]</label>
					<textcolor>$VAR[FlagsColour]</textcolor>
					<scroll>false</scroll>
					<align>right</align>
					<visible>!Skin.HasSetting(UseNonColourFlags.Enabled)</visible>
				</control>
				<control type="label">
					<posx>89</posx>
					<posy>-122</posy>
					<width>80</width>
					<height>58</height>
					<font>Font-Condensed-S36</font>
					<label fallback="                 •••">$INFO[VideoPlayer.Rating, ,]</label>
					<textcolor>mod-infotags</textcolor>
					<scroll>false</scroll>
					<align>right</align>
					<visible>Skin.HasSetting(UseNonColourFlags.Enabled)</visible>
				</control>
			</control>
			<control type="group">
				<visible>!IsEmpty(VideoPlayer.Rating) + !Skin.HasSetting(UseAlternateFlags.Enabled)</visible>
				<include>visiblehidefadeinfo</include>
				<posx>1710</posx>
				<posy>865</posy>
				<control type="image">
					<posx>2</posx>
					<posy>-116</posy>
					<width>108</width>
					<height>49</height>
					<aspect>keepaspect</aspect>
					<texture colordiffuse="$VAR[FlagsColour]">aeonflags/posters/imdb.png</texture>
					<visible>!Skin.HasSetting(UseNonColourFlags.Enabled)</visible>
				</control>
				<control type="image">
					<posx>2</posx>
					<posy>-116</posy>
					<width>108</width>
					<height>49</height>
					<aspect>keepaspect</aspect>
					<texture>aeonflags/posters/imdb.png</texture>
					<visible>Skin.HasSetting(UseNonColourFlags.Enabled)</visible>
				</control>
				<control type="label">
					<posx>89</posx>
					<posy>-122</posy>
					<width>80</width>
					<height>58</height>
					<font>Font-Condensed-S36</font>
					<label fallback="                 •••">$INFO[VideoPlayer.Rating, ,]</label>
					<textcolor>$VAR[FlagsColour]</textcolor>
					<scroll>false</scroll>
					<align>right</align>
					<visible>!Skin.HasSetting(UseNonColourFlags.Enabled)</visible>
				</control>
				<control type="label">
					<posx>89</posx>
					<posy>-122</posy>
